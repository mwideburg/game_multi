ick}></IconButton>\n            </Flex>\n        </Flex>\n    )\n}\n\nexport default Login\n","/Users/michaelwideburg/Desktop/multiGame/chat-client/src/components/DefaultPage.js",["87"],"import { IconButton } from '@chakra-ui/react'\nimport React from 'react'\nimport { BiArrowBack } from 'react-icons/bi'\nimport { Redirect, useHistory } from 'react-router-dom'\nexport default function DefaultPage() {\n    const history = useHistory()\n    const redirect = () => history.push('/')\n    return (\n        <div>\n            <IconButton mr={2} isRound='true' bg='green.300' color='white' icon={<BiArrowBack />} onClick={redirect} /> You're lost. Go home.\n        </div>\n    )\n}\n","/Users/michaelwideburg/Desktop/multiGame/chat-client/src/socketContext.js",[],["88","89"],"/Users/michaelwideburg/Desktop/multiGame/chat-client/src/usersContext.js",[],["90","91"],"/Users/michaelwideburg/Desktop/multiGame/chat-client/src/components/Game/game.js",["92","93","94","95","96","97","98","99","100","101","102","103","104","105","106","107","108","109","110","111","112","113"],"/Users/michaelwideburg/Desktop/multiGame/chat-client/src/App.js",["114"],{"ruleId":"115","replacedBy":"116"},{"ruleId":"117","replacedBy":"118"},{"ruleId":"119","severity":1,"message":"120","line":1,"column":8,"nodeType":"121","messageId":"122","endLine":1,"endColumn":16},{"ruleId":"119","severity":1,"message":"123","line":3,"column":10,"nodeType":"121","messageId":"122","endLine":3,"endColumn":13},{"ruleId":"119","severity":1,"message":"124","line":3,"column":21,"nodeType":"121","messageId":"122","endLine":3,"endColumn":28},{"ruleId":"119","severity":1,"message":"125","line":3,"column":30,"nodeType":"121","messageId":"122","endLine":3,"endColumn":40},{"ruleId":"119","severity":1,"message":"126","line":3,"column":42,"nodeType":"121","messageId":"122","endLine":3,"endColumn":46},{"ruleId":"119","severity":1,"message":"127","line":3,"column":48,"nodeType":"121","messageId":"122","endLine":3,"endColumn":52},{"rule